{
  "name": "wpdigger/wpjarvis-core",
  "description": "A Laravel-inspired, MVC-style framework for WordPress plugin development.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.0",
    "illuminate/container": "^10.0",
    "jenssegers/blade": "^1.4",
    "symfony/var-dumper": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "WPJarvis\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
